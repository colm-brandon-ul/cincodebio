    <!-- Non Atomic Classes -->
    <!-- Normal Classes -->
    <!-- WholeSlideImageCellSegmentationMask -->
    <owl:ObjectProperty rdf:about="#hasNucleusMask">
        <rdfs:label>TissueCoreNucleusMask</rdfs:label>
        <rdfs:comment>Relates a Tissue Core CellMask to it's NucleusMask</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.cincodebio.org/cdbontology#NonAtomic"/>
        <rdfs:range rdf:resource="http://www.cincodebio.org/cdbontology#Atomic"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasMembraneMask">
        <rdfs:label>MembraneMask</rdfs:label>
        <rdfs:comment>Relates a Tissue Core CellMask to it's MembraneMask</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.cincodebio.org/cdbontology#NonAtomic"/>
        <rdfs:range rdf:resource="http://www.cincodebio.org/cdbontology#Atomic"/>
    </owl:ObjectProperty>

    <owl:Class rdf:about="#WholeSlideImageCellSegmentationMask">
        <rdfs:label>WholeSlideImageCellSegmentationMask</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.cincodebio.org/cdbontology#NonAtomic"/>
        <rdfs:comment>This class describes a cell segmentation mask in a Whole Slide Image. Which contains a nucleus and membrane mask</rdfs:comment>
        <rdfs:subClassOf>
        <owl:Restriction>
            <owl:onProperty rdf:resource="http://www.cincodebio.org/cdbontology#hasNucleusMask"/>
            <owl:allValuesFrom rdf:resource="#WholeSlideImageNucleusSegmentationMask"/>
        </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
        <owl:Restriction>
            <owl:onProperty rdf:resource="http://www.cincodebio.org/cdbontology#hasMembraneMask"/>
            <owl:allValuesFrom rdf:resource="#WholeSlideImageMembraneSegmentationMask"/>
        </owl:Restriction>
