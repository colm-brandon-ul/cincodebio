    <!-- RegionsOfInterestPredicition -->

    <owl:ObjectProperty rdf:about="#hasRegionsOfInterest">
        <rdfs:label>hasRegionsOfInterest</rdfs:label>
        <rdfs:comment>Denotes a non-atomic class as having a regions of Interest </rdfs:comment>
        <rdfs:domain rdf:resource="http://www.cincodebio.org/cdbontology#NonAtomic"/>
        <rdfs:range rdf:resource="#RegionsOfInterest"/>
    </owl:ObjectProperty>

    <owl:DatatypeProperty rdf:about="#hasConfidenceThreshold">
        <rdfs:label>hasConfidenceThreshold</rdfs:label>
        <rdfs:comment>Denotes a non-atomic class as having a confidence threshold, this pertains to the mask gating process in CNN predictions.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.cincodebio.org/cdbontology#NonAtomic"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>


    <owl:Class rdf:about="#RegionsOfInterestPredicition">
        <rdfs:label>RegionsOfInterestPredicition</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.cincodebio.org/cdbontology#NonAtomic"/>
        <rdfs:comment>This class describes a prediction of regions of interest.</rdfs:comment>
        <rdfs:subClassOf>
        <owl:Restriction>
            <owl:onProperty rdf:resource="#hasRegionsOfInterest"/>
            <owl:allValuesFrom rdf:resource="#RegionsOfInterest"/>
        </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
        <owl:Restriction>
            <owl:onProperty rdf:resource="#hasConfidenceThreshold"/>
            <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>


