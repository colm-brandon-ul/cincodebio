    <!-- Files -->
    
    
    <owl:Class rdf:about="#File">
        <rdfs:label>File</rdfs:label>
        <rdfs:subClassOf rdf:resource="#SyntacticData"/>
        <rdfs:comment>This is root of all types that map to a file. The SubClasses denote what type of data the file contains such as TEXT, TABULAR, etc.. However this is not absolute as a CSV for example could be used to store Graph data, etc..</rdfs:comment>
    </owl:Class>

    <owl:DatatypeProperty rdf:about="#hasFileExtension">
    <rdfs:label>has file extension</rdfs:label>
    <rdfs:comment>This property represents the file extension of a file.</rdfs:comment>
    <rdfs:domain rdf:resource="#File"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
