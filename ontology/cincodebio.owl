    <!-- Define your classes, properties, and individuals here -->

    <owl:Class rdf:about="#Experiment">
        <rdfs:label>Experiment</rdfs:label>
        <rdfs:comment>The root representing Experiments in CincoDeBio. An experiment is the collection of raw data produced by an experiment, this facilitates automated generation of upload forms and file management.</rdfs:comment>
    </owl:Class>


    <owl:Class rdf:about="#Service">
        <rdfs:label>Service</rdfs:label>
        <rdfs:comment>The root representing a Service in CincoDeBio. This is any data processing component, a single step in a workflow.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#ServiceConcept">
        <rdfs:label>ServiceConcept</rdfs:label>
        <rdfs:comment>The root representing a Service Concept in CincoDeBio. The Subclasses of this class describes what a I/O tuple pairs, a service instance should conform to, in order to be part of that concept.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#Automated">
        <rdfs:label>Automated</rdfs:label>
        <rdfs:comment>This represents any automated Service in CincoDeBio, this is a Service which runs to completion without human intervention.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Service"/>
    </owl:Class>

    <owl:Class rdf:about="#Interactive">
        <rdfs:label>Interactive</rdfs:label>
        <rdfs:comment>This represents any interactive Service in CincoDeBio, this is a Service which requires human intervention at runtime.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Service"/>
    </owl:Class>

    <owl:AllDisjointClasses>
    <owl:members rdf:parseType="Collection">
        <rdf:Description rdf:about="#Automated"/>
        <rdf:Description rdf:about="#Interactive"/>
    </owl:members>
    </owl:AllDisjointClasses>

     <owl:Class rdf:about="#Data">
        <rdfs:label>Data</rdfs:label>
        <rdfs:comment>This is root of all types in CdB.</rdfs:comment>
    </owl:Class>

    <!-- Semantic Data -->

    <owl:Class rdf:about="#SemanticData">
        <rdfs:label>SemanticData</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Data"/>
        <rdfs:comment>The root for all SemanticData Data in CincoDeBio. All Data Structures in the Application Domain Ontology Inherit from this.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#Atomic">
        <rdfs:label>Atomic</rdfs:label>
        <rdfs:subClassOf rdf:resource="#SemanticData"/>
        <rdfs:comment>This represents an atomic data type. Which resolves to either a single File or a Primitive.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#NonAtomic">
        <rdfs:label>NonAtomic</rdfs:label>
        <rdfs:subClassOf rdf:resource="#SemanticData"/>
        <rdfs:comment>This represents a non-atomic data type. Which resolves to some Data Structure.</rdfs:comment>
    </owl:Class>

    <!-- Syntactic Data/Files -->


    <owl:Class rdf:about="#SyntacticData">
        <rdfs:label>SyntacticData</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Data"/>
        <rdfs:comment>The root for all Syntactic Data in CincoDeBio. This is inherited from for defining any generic data structure or file type.</rdfs:comment>
    </owl:Class>

